{

  "detectors": [
    "problem.asm.AdapterDetector",
    "problem.asm.DecoratorDetector",
    "problem.asm.SingletonDetector",
    "problem.asm.CompositeDetector"
  ],

  "dot_output": "input_output/our_project.dot",
  "png_output": "input_output/our_project.png",

  "packages": [
    "./src/problem/asm"
  ],
  "classes": [
    "org.objectweb.asm.ClassVisitor"
  ],

  "adapterSettings": {
    "minimumMethodCalls": 6
  },

  "phases": [
    "problem.asm.PhaseClassGeneration",
    "problem.asm.PhaseNodeGeneration",
    "problem.asm.PhaseDecoration",
    "problem.asm.PhaseWrite",
    "problem.asm.RunDotExecutable"
  ]
}